<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Poplection.AddNewSet">
    <ContentPage.Content>
        <StackLayout WidthRequest="200" HorizontalOptions="Center">
            <Label x:Name="SetNameLabel" Text="Setname:"/>
            <Entry x:Name="SetNameInput" Placeholder="Setname"/>
            <Label x:Name="SetImageURLLabel" Text="Image URL:"/>
            <Entry x:Name="SetImageURLInput"/>
            <Button x:Name="AddSetButton" Text="Add" BackgroundColor="{StaticResource Primary}" TextColor="{StaticResource FontColorWhite}" Clicked="AddSetButton_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>